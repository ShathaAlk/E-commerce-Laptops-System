<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta name="description" content="Aroma">
    <meta name="keywords" content="">
    <meta name="author" content="tabthemes">

    <!-- Favicons -->
    <link rel="shortcut icon" href="~/assets/img/logo_icon.png">
    <link rel="apple-touch-icon" sizes="57x57" href="~/assets/img/logo_icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/assets/img/logo_icon.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/assets/img/logo_icon.png">

    <!-- Google Web Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet" />

    <!-- Bootstrap CSS -->
    <link href="~/assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- CSS Files For Plugin -->
    <link href="~/assets/css/animate.css" rel="stylesheet">
    <link href="~/assets/css/font-awesome/font-awesome.min.css" rel="stylesheet">
    <link href="~/assets/css/magnific-popup.css" rel="stylesheet" />
    <link href="~/assets/css/YTPlayer.css" rel="stylesheet" />
    <link href="~/assets/inc/owlcarousel/css/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/assets/inc/owlcarousel/css/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/assets/inc/revolution/css/settings.css" rel="stylesheet" />
    <link href="~/assets/inc/revolution/css/layers.css" rel="stylesheet" />
    <link href="~/assets/inc/revolution/css/navigation.css" rel="stylesheet" />

    <!-- Custom CSS -->
    <link href="~/assets/css/style.css" rel="stylesheet">
    <link href="~/assets/css/popup.css" rel="stylesheet">
    <!--validation bootstrap-->
    <link rel="stylesheet" href="~/assets/vendor/bootstrap/bootstrap.min.css">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <!-- Preloader -->
    <div id="preloader">
        <div id="spinner"></div>
    </div>
    <!-- End Preloader-->

    <div>
        @RenderBody()

    </div>
    <!-- pop-up Message -->
    <div id="divModal" class="modal">
        <!-- modal content -->
        <div class="modal-content">
            <div class="modal-body justify-content-center text-center">
                <img class="pop-icon" src="~/assets/img/icon/x-mark.png" />
                <br />
                @if (TempData["EmailExisted"] != null)
                {
                    <!-- Email existed message -->
                    @Html.Raw(TempData["EmailExisted"])
                    <span>This Email Address Already Exists.</span>
                }
                else if (TempData["LoginFailed"] != null)
                {
                    <!-- Login failed message -->
                    @Html.Raw(TempData["LoginFailed"])
                    <span>Login Failed, Email or Password is Incorrect.</span>
                }

                <br />
                <span class="btn btn-main btn-theme fadeInUp ok m-top-10">OK</span>
            </div>
        </div>
    </div>
    <!-- /pop-up Message -->
    <!-- jQuery -->
    <script src="~/assets/js/jquery.min.js"></script>

    <!-- Bootstrap -->
    <script src="~/assets/bootstrap/js/bootstrap.min.js"></script>

    <!-- Components Plugin -->
    <script src="~/assets/js/jquery.easing.1.3.js"></script>
    <script src="~/assets/js/smooth-scroll.js"></script>
    <script src="~/assets/js/jquery.appear.js"></script>
    <script src="~/assets/js/jquery.countTo.js"></script>
    <script src="~/assets/js/jquery.stellar.min.js"></script>
    <script src="~/assets/js/jquery.magnific-popup.min.js"></script>
    <script src="~/assets/js/imagesloaded.pkgd.min.js"></script>
    <script src="~/assets/js/isotope.pkgd.min.js"></script>
    <script src="~/assets/js/jquery.mb.YTPlayer.js"></script>
    @*<script src="~/assets/js/retina.min.js"></script>*@
    <script src="~/assets/js/wow.min.js"></script>
    <script src="~/assets/inc/owlcarousel/js/owl.carousel.min.js"></script>
    <script src="~/assets/inc/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/assets/inc/revolution/js/jquery.themepunch.revolution.min.js"></script>

    <!-- Contact Form -->
    <script src="~/assets/js/contact.js"></script>

    <!-- Custom Plugin -->
    <script src="~/assets/js/custom.js"></script>

    <!-- RS Plugin Extensions -->
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.video.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.carousel.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.actions.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.kenburn.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.navigation.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.migration.min.js"></script>
    <script src="~/assets/inc/revolution/js/extensions/revolution.extension.parallax.min.js"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>*@
    <!--validation bootstrap-->
    <script src="~/assets/vendor/bootstrap/bootstrap.min.js"></script>
    <!--pop up-->
    <script src="~/assets/js/popup.js"></script>
    <!--form validation-->
    <script src="~/assets/js/form-validation.js"></script>
</body>
</html>